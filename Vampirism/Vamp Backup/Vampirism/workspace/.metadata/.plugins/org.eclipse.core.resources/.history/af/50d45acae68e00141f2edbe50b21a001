package vampirism;

public class farm extends building {
	public farm(int level) {
		
		switch (level) {
		case 1: 	cost = 210;
					income = 3;
					break;
		case 2:   	cost = 400;
					income = 7;
					//biodome lvl 2 or ancient tree required
					break;
		case 3:   	cost = ;
					income = 7;
					break;
					
		}
			
	}

}
